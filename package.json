{
  "name": "icbs-mobil-front-responsive",
  "version": "1.9.12",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "build-bavv": "vite build --mode bavv --base=/BAVV/icbs-web-resp/",
    "build-bpop": "vite build --mode bpop --base=/BPOP/icbs-web-resp/",
    "build-bocc": "vite build --mode bocc --base=/BOCC/icbs-web-resp/",
    "build-bbog": "vite build --mode bbog --base=/BBOG/icbs-web-resp/",
    "build-g3-bavv": "vite build --mode g3-bavv --base=/BAVV/icbs-web-resp/",
    "build-g3-bpop": "vite build --mode g3-bpop --base=/BPOP/icbs-web-resp/",
    "build-g3-bocc": "vite build --mode g3-bocc --base=/BOCC/icbs-web-resp/",
    "build-g3-bbog": "vite build --mode g3-bbog --base=/BBOG/icbs-web-resp/",
    "build-g2-bavv": "vite build --mode g2-bavv --base=/BAVV/icbs-web-resp/",
    "build-g2-bpop": "vite build --mode g2-bpop --base=/BPOP/icbs-web-resp/",
    "build-g2-bocc": "vite build --mode g2-bocc --base=/BOCC/icbs-web-resp/",
    "build-g2-bbog": "vite build --mode g2-bbog --base=/BBOG/icbs-web-resp/",
    "build-prod-bavv": "vite build --mode prod-bavv --base=/BAVV/icbs-web-resp/",
    "build-prod-bpop": "vite build --mode prod-bpop --base=/BPOP/icbs-web-resp/",
    "build-prod-bocc": "vite build --mode prod-bocc --base=/BOCC/icbs-web-resp/",
    "build-prod-bbog": "vite build --mode prod-bbog --base=/BBOG/icbs-web-resp/",
    "build-qa-bavv": "vite build --mode qa-bavv --base=/BAVV/icbs-web-resp/",
    "build-qa-bpop": "vite build --mode qa-bpop --base=/BPOP/icbs-web-resp/",
    "build-qa-bocc": "vite build --mode qa-bocc --base=/BOCC/icbs-web-resp/",
    "build-qa-bbog": "vite build --mode qa-bbog --base=/BBOG/icbs-web-resp/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.20.5",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vuepic/vue-datepicker": "^5.3.0",
    "axios": "^1.2.1",
    "jsencrypt": "^3.3.1",
    "moment": "^2.29.4",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue3-lottie": "^3.0.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^4.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "typescript": "~4.7.4",
    "vite": "^4.3.9",
    "vitest": "^0.24.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^1.0.8"
  }
}
